{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 21,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "kr_auto_checkpoint_veloc",
      "hidden": true,
      "cacheVariables": {
        "KR_VELOC_BAREBONE": {
          "type": "BOOL",
          "value": "ON"
        },
        "KR_ENABLE_VELOC": {
          "type": "BOOL",
          "value": "ON"
        },
        "KR_ENABLE_STDFILE": {
          "type": "BOOL",
          "value": "ON"
        }
      }
    },
    {
      "name": "kr_debug",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "KR_ENABLE_TRACING": {
          "type": "BOOL",
          "value": "ON"
        }
      }
    },
    {
      "name": "kr_release",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "kr_relwithdebinfo",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo",
        "KR_ENABLE_TRACING": {
          "type": "BOOL",
          "value": "ON"
        }
      }
    }
  ]
}
